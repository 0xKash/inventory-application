<html>
  <body>
    <h1>Inventory:</h1>
    <a href="/create"><span>Create item</span></a>
    <hr />
    <% inventory.forEach(element => { %>
    <div>
      <h1><%= element.game_name %></h1>
      <h2>Genre: <%= element.game_genre %></h2>
      <h2>Developer: <%= element.game_developer %></h2>
      <p>Quantity: x<%= element.game_quantity %></p>
      <button>Remove Item</button>
    </div>
    <hr />
    <% }); %>
  </body>
</html>
